{
  "name": "Highchart",
  "version": "0.2.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "favicon.png"
  },
  "icons": {
    "128": "favicon.png"
  },
  "host_permissions": ["http://*/*", "https://*/*", "*://localhost/*"],
  "permissions": [
    "notifications",
    "tabs",
    "management",
    "cookies",
    "storage",
    "activeTab",
    "system.memory",
    "system.cpu",
    "browsingData",
    "desktopCapture",
    "tabCapture",
    "identity",
    "input"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "run_at": "document_start",
      "all_frames": true,
      "js": ["content.js"]
    }
  ],
  "devtools_page": "devtools.html"
}
